{
    "name"             : "friendsofvictoire/articlelist-widget",
    "type"             : "symfony-bundle",
    "description"      : "Victoire DCMS List articles widget",
    "keywords"         : [
        "DCMS",
        "Victoire",
        "widget",
        "FriendsofVictoire",
        "FoV",
        "Dynamic",
        "CMS",
        "article",
        "list",
        "blog"
    ],
    "homepage"         : "http://victoire.io",
    "license"          : "MIT",
    "authors"          : [
        {
            "name" : "Leny Bernard",
            "email": "leny@victoire.io"
        }
    ],
    "require"          : {
        "php"                             : ">=5.3.3",
        "symfony/framework-bundle"        : "~2.3",
        "victoire/victoire"               : "~1.3",
        "lexik/form-filter-bundle"        : "3.*@dev",
        "friendsofvictoire/listing-widget": "1.3.x-dev"
    },
    "autoload"         : {
        "psr-0": {
            "Victoire\\Widget\\ArticleListBundle": ""
        }
    },
    "minimum-stability": "dev",
    "target-dir"       : "Victoire/Widget/ArticleListBundle",
    "extra"            : {
        "branch-alias": {
            "dev-master": "1.3.x-dev"
        }
    }
}
